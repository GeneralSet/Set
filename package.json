  "repository": null,
  "files": [
    "pkg/node/set_bg.wasm",
    "pkg/node/set.d.ts",
    "pkg/node/set.js",
    "pkg/web/set_bg.wasm",
    "pkg/web/set.d.ts",
    "pkg/web/set.js"
  ],
  "main": "pkg/set.js",
  "scripts": {
    "build-wasm": "cargo build --target wasm32-unknown-unknown --release",
    "build-web": "wasm-bindgen target/wasm32-unknown-unknown/release/set.wasm --out-dir pkg/web --typescript --browser",
    "build-node": "wasm-bindgen target/wasm32-unknown-unknown/release/set.wasm --out-dir pkg/node --typescript --nodejs",
    "preserve": "npm run build-wasm && npm run build-web",
    "serve": "cd demo && webpack-dev-server",
    "pretest": "npm run build-wasm && npm run build-node",
    "test": "cd tests && yarn install && yarn test"
  }
}
